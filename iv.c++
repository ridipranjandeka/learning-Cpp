/*c++ program to take length and breadth of a rectangle and print its area*/
#include<iostream>
using namespace std;
int main()
{
    int length = 10;
    int breadth = 20;
    cout<<"Length"<<length<<endl;
    cout<<"Breadth"<<breadth<<endl;
    int area = length*breadth;
    cout<<"Area is "<<area<<endl;
    return 0;
}